{
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "rules": {
    "quotes": [2, "double"],
    "object-curly-spacing": "off",
    "comma-dangle": "off",
    "operator-linebreak": "off",
    "indent": "off",
    "require-jsdoc": "off"
  },
  "extends": ["eslint:recommended", "google"]
}
